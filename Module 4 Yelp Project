-- 6. Find the distribution of star ratings to the business in the following cities:
SELECT stars, COUNT(*)
FROM business
WHERE city = 'Avon'
GROUP BY stars;

-- +-------+----------+
-- | stars | COUNT(*) |
-- +-------+----------+
-- |   1.5 |        1 |
-- |   2.5 |        2 |
-- |   3.5 |        3 |
-- |   4.0 |        2 |
-- |   4.5 |        1 |
-- |   5.0 |        1 |
-- +-------+----------+

SELECT stars, COUNT(*)
FROM business
WHERE city = 'Beachwood'
